{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Uvicorn",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"src.main:app",
				"--host",
				"0.0.0.0",
				"--port",
				"8000",
				"--ws",
				"websockets",
			],
			"sudo": true,
			"jinja": true,
			"justMyCode": false,
			"env": {
				"PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
				"LOG_LEVEL": "DEBUG"
			}
		},
		{
			"name": "Python: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": false,
			"env": {
				"PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"
			}
		},
		{
			"name": "Advanced Debug in Supervisely platform",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"src.main:m.app",
				"--host",
				"0.0.0.0",
				"--port",
				"8000",
				"--ws",
				"websockets"
			],
			"jinja": true,
			"justMyCode": true,
			"env": {
				"PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
				"LOG_LEVEL": "DEBUG",
				"ENV": "production",
				"DEBUG_WITH_SLY_NET": "1",
				"SLY_APP_DATA_DIR": "${workspaceFolder}/app_data",
				"TEAM_ID": "473"
			}
		}
	]
}